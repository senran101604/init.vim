{
"codeLens.enable": true,
"coc.preferences.colorSupport": true,
// nerd tree icons
"suggest.completionItemKindLabels": {
    "method": "  ",
    "function": "  ",
    "variable": "[]",
    "field": "  ",
    "typeParameter": "<>",
    "constant": "  ",
    "class": " פּ ",
    "interface": " 蘒",
    "struct": "  ",
    "event": "  ",
    "operator": "  ",
    "module": "  ",
    "property": "  ",
    "enum": " 練",
    "reference": "  ",
    "keyword": "  ",
    "file": "  ",
    "folder": " ﱮ ",
    "color": "  ",
    "unit": " 塞 ",
    "snippet": "  ",
    "text": "  ",
    "constructor": "  ",
    "value": "  ",
    "enumMember": "  "
  },
// python
"python.linting.enabled":false,
// coc-clangd
"clangd.disableDiagnostics":true,
// coc-yank
"yank.enableCompletion":false,
"suggest.autoTrigger": "none",
// Rust
"workspace.ignoredFolders": [
  "$HOME",
  "$HOME/.cargo/**",
  "$HOME/.rustup/**"
],
"rust-analyzer.checkOnSave": false,
"rust-analyzer.diagnostics.enable":false,
"rust-analyzer.lens.enable": false,
"languageserver": {
  "rust-analyzer": {
    "enable": true,
    "features": {
      "checkOnSave": false,
      "highlight": false,
      "inlayHints": false,
      "rainbowHighlight": false,
      "referencesCodeLens": false,
      "rename": false,
      "run": false,
      "signatureHelp": false,
      "fullSyntaxTree": false,
      "hoverActions": false,
      "completion": false,
      "gotoDefinition": true,
      "gotoImplementation": false,
      "findImplementations": false,
      "findReferences": false,
      "hover": true
    }
  }
}

}
